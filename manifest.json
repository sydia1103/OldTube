{
  "name": "OldTubeV2 mod by alitek12",
  "version": "0.0.3",
  "manifest_version": 2,
  "content_scripts": [ {
      "js": [ "content.js"],
      "matches": [ "*://www.youtube.com/*" ],
      "run_at": "document_start"
  }],
  "description": "Restore Old Youtube",
  "homepage_url": "http://example.com",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": ["https://www.youtube.com/*", "webRequest", "webRequestBlocking"]
}